{
  "hosting": {
    "public": "src/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "functions/firebase-debug.log"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "app"
      }
    ]
  },
  "functions": {
    "source": "src/functions"
  }
}
